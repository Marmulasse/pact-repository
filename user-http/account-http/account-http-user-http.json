{
    "provider": {
        "name": "user-http"
    },
    "consumer": {
        "name": "account-http"
    },
    "interactions": [
        {
            "description": "Should return a user",
            "request": {
                "method": "GET",
                "path": "/users/123"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json; charset=UTF-8"
                },
                "body": {
                    "username": "fJJGKVZoqVfPoVkDQZpN"
                },
                "matchingRules": {
                    "$.body.username": {
                        "match": "type"
                    }
                }
            }
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.3.7"
        }
    }
}